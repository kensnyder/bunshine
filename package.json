{
  "name": "bunshine",
  "version": "1.0.1",
  "module": "server/server.ts",
  "type": "module",
  "main": "index.ts",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test-watch": "bun test --watch",
    "coverage": "bun test --coverage",
    "lint": "tsc",
    "build": "bun run build:dts && bun run build:mimes",
    "build:dts": "rm index.d.ts; bunx dts-bundle-generator -o index.d.ts index.ts",
    "build:mimes": "bun scripts/compileCompressibleMimes.ts",
    "example": "bun --watch ./examples/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/bunshine.git"
  },
  "bin": {
    "serve": "./bin/serve.ts"
  },
  "keywords": [
    "Bun HTTP Server",
    "Bun Socket Server",
    "Bun Server",
    "Bun Framework",
    "Bun Web Framework",
    "Bun Server Sent Events",
    "Bun Ranged Files",
    "Bun File Server",
    "HTTP Server",
    "Socket Server",
    "Server",
    "Server Sent Events",
    "Ranged Files",
    "File Server",
    "GZIP Responses",
    "Bun"
  ],
  "author": "kendsnyder@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kensnyder/bunshine/issues"
  },
  "homepage": "https://github.com/kensnyder/bunshine#readme",
  "dependencies": {
    "@types/mime-db": "1.43.5",
    "lru-cache": "11.0.0",
    "path-to-regexp": "6.2.2"
  },
  "devDependencies": {
    "@types/eventsource": "1.1.15",
    "@types/ms": "0.7.34",
    "bun-types": "1.1.20",
    "eventsource": "2.0.2",
    "globby": "14.0.2",
    "mime-db": "1.53.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "tinybench": "2.8.0",
    "type-fest": "4.21.0",
    "typescript": "5.5.3"
  }
}
